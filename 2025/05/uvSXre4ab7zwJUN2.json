        "functionCode": "const msg = $json?.messages?.[0] || {};\nconst fromMe = msg?.key?.fromMe ?? false;\nconst bodyMsg = msg?.message?.conversation || '';\n\nif (fromMe || !bodyMsg.trim()) {\n  return [{ json: { skip: true, reason: 'Mensagem vazia ou enviada pelo bot' } }];\n}\n\nconst remoteJid = msg?.key?.remoteJid || '';\nconst number = remoteJid.replace(/[^0-9]/g, '');\nconst sessionId = msg?.key?.id || $execution.id;\n\nreturn [{\n  json: {\n    chatInput: bodyMsg.trim(),\n    sessionId,\n    number,\n    message: bodyMsg.trim()\n  }\n}];"
